{
	"extends": [
		"eslint:recommended",
		"plugin:import/errors",
		"plugin:jsx-a11y/recommended"
	],
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"plugins": ["import", "jsx-a11y"],
	"rules": {
		"import/no-webpack-loader-syntax": "off",
		"react/prop-types": "off",
		"no-extra-semi": "off",
		"no-unused-vars": "off"
	},
	"env": {
		"browser": true
	}
}
